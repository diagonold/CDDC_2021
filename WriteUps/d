# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
# Allows us to open eclipse and run it in the background with &
alias eclipse='/home/ryansacatani/eclipse/jee-2019-06/eclipse/eclipse &'
# Allows us to call EyeExcercise script and run it in the background while working
alias eyeExcercise='/home/ryansacatani/Documents/Bash_Projects/EyeExcercise.sh &'
# Allow us to use xclip and use ctrl+v to paste txt.
# This just means we have to be aware when using the xclip and other paste options
alias xclip='xclip -sel clip'
# This allows us to run john the ripper password cracker from any command line
alias john="/home/ryansacatani/Documents/hacking/Utility_scripts/john/run/john"
# This allows us to access SUTD term_5 stuff by just typing school
alias school="cd /home/ryansacatani/Documents/SUTD/Term_8"
# For seraching exploit data base on command line with searchSploit
alias searchsploit="/opt/exploit-database/searchsploit"
# Opening autopsy from the commandLine
alias autopsy="/home/ryansacatani/Desktop/autopsy-4.15.0/bin/autopsy"
# Add an "alert" alias for long running commands.  Use like so:
