# Allows us to write studio and access android studio from anywhere
alias studio="/home/ryansacatani/Downloads/android-studio/bin/studio.sh &"

# Allow us to open Mendeley by typing mendeley
alias mendeley="/home/ryansacatani/Desktop/mendeleydesktop-1.19.8-linux-x86_64/bin/mendeleydesktop"

alias dirbuster="source /opt/dirbuster/DirBuster-1.0-RC1.sh"